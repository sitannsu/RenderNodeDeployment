        {
          "name": "Accept Patient Query",
          "request": {
            "method": "PATCH",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{doctor_token}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"doctorResponse\": \"Thank you for reaching out. Based on your symptoms of chest pain and shortness of breath, I recommend an immediate in-person consultation. These symptoms could indicate a cardiac issue that requires urgent evaluation. Please schedule an appointment as soon as possible.\",\n  \"status\": \"accepted\",\n  \"appointmentTime\": \"2024-01-20T14:00:00.000Z\",\n  \"consultationType\": \"in-person\",\n  \"additionalNotes\": \"Please bring your recent ECG reports, any previous cardiac test results, and a list of current medications. Avoid strenuous activity until our consultation. If symptoms worsen, go to the emergency room immediately.\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{base_url}}/api/doctors/queries/{{query_id}}/reply",
              "host": ["{{base_url}}"],
              "path": ["api", "doctors", "queries", "{{query_id}}", "reply"]
            },
            "description": "Accept a patient query with detailed medical advice, appointment scheduling, and additional instructions. Patient receives automatic notification."
          },
          "response": []
        },
        {
          "name": "Reject Patient Query",
          "request": {
            "method": "PATCH",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{doctor_token}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"doctorResponse\": \"Thank you for your query. Based on the symptoms you've described (chest pain and breathing difficulties), I believe this requires immediate emergency medical attention rather than a scheduled consultation. I strongly recommend visiting your nearest emergency department immediately.\",\n  \"status\": \"rejected\",\n  \"additionalNotes\": \"This is an emergency situation that requires immediate hospital care. Please do not delay - go to the nearest emergency room or call emergency services.\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{base_url}}/api/doctors/queries/{{query_id}}/reply",
              "host": ["{{base_url}}"],
              "path": ["api", "doctors", "queries", "{{query_id}}", "reply"]
            },
            "description": "Decline a patient query with medical reasoning and alternative recommendations. Useful when the case is outside doctor's expertise or requires emergency care."
          },
          "response": []
        },
        {
          "name": "Complete Patient Query",
          "request": {
            "method": "PATCH",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{doctor_token}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"doctorResponse\": \"Thank you for following up on your chest pain concern. Based on our consultation and the test results you provided, your condition has improved significantly. The ECG shows normal rhythm and the chest X-ray appears clear. Please continue with the prescribed medication for another week and maintain a healthy lifestyle with regular exercise and a balanced diet.\",\n  \"status\": \"completed\",\n  \"additionalNotes\": \"Follow-up appointment scheduled in 2 weeks if needed. Continue current treatment plan. Contact me immediately if you experience any recurring symptoms. Lifestyle recommendations: avoid smoking, limit caffeine, maintain regular sleep schedule.\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{base_url}}/api/doctors/queries/{{query_id}}/reply",
              "host": ["{{base_url}}"],
              "path": ["api", "doctors", "queries", "{{query_id}}", "reply"]
            },
            "description": "Mark a patient query as completed with final medical advice and follow-up instructions. Used when the consultation is finished and no further action is needed."
          },
          "response": []
        },
