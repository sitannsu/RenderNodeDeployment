    {
      "name": "ðŸ©º Patient Query Management",
      "description": "APIs for patients to create, view, and manage their medical queries to doctors",
      "item": [
        {
          "name": "Create Patient Query",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "// Save query ID for use in other requests",
                  "if (pm.response.code === 201) {",
                  "    const response = pm.response.json();",
                  "    if (response._id) {",
                  "        pm.collectionVariables.set('query_id', response._id);",
                  "        console.log('Query ID saved:', response._id);",
                  "    }",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{patient_token}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"doctorId\": \"{{doctor_id}}\",\n  \"symptoms\": \"I have been experiencing severe chest pain and shortness of breath for the past 2 days. The pain is sharp and occurs especially when I take deep breaths or move around. Sometimes I feel dizzy and nauseous.\",\n  \"duration\": \"2 days\",\n  \"previousTreatments\": \"I took paracetamol and rested, but the pain persists. I also tried applying a heating pad but it didn't help much.\",\n  \"attachments\": [\n    {\n      \"url\": \"https://example.com/chest-xray.jpg\",\n      \"description\": \"Chest X-ray taken at local clinic yesterday\"\n    },\n    {\n      \"url\": \"https://example.com/ecg-report.pdf\",\n      \"description\": \"ECG report showing irregular heartbeat\"\n    }\n  ],\n  \"preferredTime\": \"Morning (9 AM - 12 PM) or Evening (6 PM - 8 PM)\",\n  \"consultationType\": \"online\",\n  \"subject\": \"Urgent chest pain and breathing difficulty\",\n  \"urgency\": \"high\",\n  \"patientContactNo\": \"+911234567890\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{base_url}}/api/patient/queries",
              "host": ["{{base_url}}"],
              "path": ["api", "patient", "queries"]
            },
            "description": "Create a new medical query to a specific doctor. Includes symptoms, medical history, attachments, and consultation preferences."
          },
          "response": []
        },
