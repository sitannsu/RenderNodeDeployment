    {
      "name": "ðŸ§ª Test Scenarios",
      "description": "Pre-configured test scenarios for common workflows",
      "item": [
        {
          "name": "Scenario 1: Patient Creates Query â†’ Doctor Accepts",
          "item": [
            {
              "name": "Step 1: Search for Cardiologist",
              "event": [
                {
                  "listen": "test",
                  "script": {
                    "exec": [
                      "// Auto-save first doctor ID",
                      "if (pm.response.code === 200) {",
                      "    const response = pm.response.json();",
                      "    if (response.doctors && response.doctors.length > 0) {",
                      "        pm.collectionVariables.set('doctor_id', response.doctors[0]._id);",
                      "    }",
                      "}",
                      "",
                      "// Auto-run next request after 1 second",
                      "setTimeout(() => {",
                      "    postman.setNextRequest('Step 2: Create Urgent Query');",
                      "}, 1000);"
                    ],
                    "type": "text/javascript"
                  }
                }
              ],
              "request": {
                "method": "GET",
                "header": [],
                "url": {
                  "raw": "{{base_url}}/api/patient/doctors/search?specialization=Cardiology&limit=5",
                  "host": ["{{base_url}}"],
                  "path": ["api", "patient", "doctors", "search"],
                  "query": [
                    {
                      "key": "specialization",
                      "value": "Cardiology"
                    },
                    {
                      "key": "limit",
                      "value": "5"
                    }
                  ]
                }
              },
              "response": []
            },
            {
              "name": "Step 2: Create Urgent Query",
              "event": [
                {
                  "listen": "test",
                  "script": {
                    "exec": [
                      "// Auto-save query ID",
                      "if (pm.response.code === 201) {",
                      "    const response = pm.response.json();",
                      "    if (response._id) {",
                      "        pm.collectionVariables.set('query_id', response._id);",
                      "    }",
                      "}",
                      "",
                      "// Auto-run next request after 2 seconds",
                      "setTimeout(() => {",
                      "    postman.setNextRequest('Step 3: Doctor Views Query');",
                      "}, 2000);"
