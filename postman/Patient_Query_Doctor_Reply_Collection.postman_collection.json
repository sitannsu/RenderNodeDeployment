    {
      "name": "ðŸ‘¨â€âš•ï¸ Doctor Query Management",
      "description": "APIs for doctors to view, respond to, and manage patient queries",
      "item": [
        {
          "name": "Get All Patient Queries for Doctor",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "// Test if endpoint returns correct structure",
                  "pm.test('Status code is 200', function () {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "",
                  "pm.test('Response has correct structure', function () {",
                  "    const jsonData = pm.response.json();",
                  "    pm.expect(jsonData).to.have.property('success');",
                  "    pm.expect(jsonData).to.have.property('data');",
                  "    pm.expect(jsonData.data).to.have.property('queries');",
                  "});",
                  "",
                  "pm.test('Queries contain patient data only', function () {",
                  "    const jsonData = pm.response.json();",
                  "    if (jsonData.data.queries.length > 0) {",
                  "        const query = jsonData.data.queries[0];",
                  "        pm.expect(query).to.have.property('patient');",
                  "        pm.expect(query.patient).to.have.property('name');",
                  "        pm.expect(query).to.not.have.property('homeAddress');",
                  "        pm.expect(query).to.not.have.property('hospitalAddress1');",
                  "    }",
                  "});",
                  "",
                  "console.log('Response:', JSON.stringify(pm.response.json(), null, 2));"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{doctor_token}}"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/doctors/queries?status=pending&page=1&limit=10&sortBy=createdAt&sortOrder=desc",
              "host": ["{{base_url}}"],
              "path": ["api", "doctors", "queries"],
              "query": [
                {
                  "key": "status",
                  "value": "pending",
                  "description": "Filter by status: pending, accepted, rejected, completed"
                },
                {
                  "key": "page",
                  "value": "1",
                  "description": "Page number for pagination"
                },
                {
                  "key": "limit",
                  "value": "10",
                  "description": "Number of queries per page"
                },
                {
                  "key": "sortBy",
                  "value": "createdAt",
                  "description": "Field to sort by (createdAt, urgency, status)"
                },
                {
                  "key": "sortOrder",
                  "value": "desc",
                  "description": "Sort order: asc or desc"
                }
              ]
            },
            "description": "Get all patient queries sent to the authenticated doctor with essential information for dashboard view. Returns: patient name/ID, query subject, symptoms (truncated), urgency, status, timing, and response status - optimized for doctor dashboard listing."
          },
          "response": []
